{
    "manifestVersion": 1,
    "id": "ispw-source-code-downloader",
    "name": "BMC AMI DevX Source Code Downloader",
    "public": true,
    "version": "1.1.0",
    "publisher": "BMC",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Extension 1.1.0 used for downloading source from Code Pipeline Repository and Containers",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/ISPW_marble.png"        
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/bmc-compuware/ispw-source-code-downloader-azure-extension"
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/bmc-compuware/ispw-source-code-downloader-azure-extension/master/README.md"
        },
        "support": {
            "uri": "https://support.bmc.com"     
        },
        "license":{
            "uri":"https://github.com/bmc-compuware/ispw-source-code-downloader-azure-extension/master/LICENSE"
        }

    },
    "content": {
        "details": {
            "path": "README.md"
        },
		"license": {
            "path": "LICENSE"
        }
		
    },
    "tags": [
        "BMC",
        "AMI",
        "DevX",
        "ISPW",
        "Source Code Downloader",
        "Mainframe",
        "Continuous Integration",
        "Code Pipeline"
    ],
    "files": [
        {
            "path": "downloadertask"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "downloadertask"
            }
        }
    ]
}